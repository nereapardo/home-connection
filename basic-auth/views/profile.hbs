<main class="profile">
    <h1> Welcome {{user.username}}</h1>
    <h3>{{user.contact}}</h3>
    <a href="/new-house">New house</a>
    {{#each userHouses}}
        {{this.title}}
        <a href="/house/{{this._id}}/update">Edit</a>
        <form method="POST" action="/house/{{this._id}}/delete">
            <button type="submit">Delete</button>
        </form>
    {{/each}}
</main>
